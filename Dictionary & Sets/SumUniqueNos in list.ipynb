    "#find the sum of unique numbers in the list\n",
    "\n",
    "def sumUnique(l):\n",
    "    \n",
    "    s = set()\n",
    "    for i in l:\n",
    "        s.add(i)\n",
    "    sum = 0\n",
    "    for i in s:\n",
    "        sum = sum + i\n",
    "    return sum\n",
    "\n",
    "ans= sumUnique([1,2,1,3,4,2,5,4])\n",
    "print(ans)"
   ]
  }
